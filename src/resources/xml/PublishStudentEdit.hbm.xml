<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.kp.cms.bo.admission.PublishStudentEdit"
		table="publish_for_student_edit">
		<comment></comment>
		<id name="id" type="int">
			<column name="id" />
			<generator class="native" />
		</id>
		 <many-to-one name="student" class="com.kp.cms.bo.admin.Student" fetch="select" >
            <column name="student_id">
                <comment></comment>
            </column>
        </many-to-one>
		<many-to-one name="classes" class="com.kp.cms.bo.admin.Classes" fetch="select" >
            <column name="class_id">
                <comment></comment>
            </column>
        </many-to-one>
		
		<property name="createdBy" update="false">
			<column name="created_by">
				<comment></comment>
			</column>
		</property>
		<property name="createdDate" update="false">
			<column name="created_date">
				<comment></comment>
			</column>
		</property>
		<property name="modifiedBy">
			<column name="modified_by">
				<comment></comment>
			</column>
		</property>
		<property name="lastModifiedDate">
			<column name="last_modified_date">
				<comment></comment>
			</column>
		</property>
		<property name="isActive"
				  column="is_active" 
				  type="java.lang.Boolean"/>
				  
		<property name="startDate">
			<column name="start_date">
				<comment></comment>
			</column>
		</property>
		<property name="endDate">
			<column name="end_date">
				<comment></comment>
			</column>
		</property>
	</class>
</hibernate-mapping>